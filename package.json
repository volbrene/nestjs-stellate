{
  "name": "nestjs-stellate",
  "version": "1.0.0",
  "description": "Stellate.co Module for Nest Framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/volbrene/nestjs-stellate.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "test": "npm run test:e2e",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "start": "ts-node -r tsconfig-paths/register example/src/main.ts",
    "lint:prettier": "prettier --check .",
    "prepare": "volbrene-git-hooks"
  },
  "keywords": [
    "nestjs",
    "stellate",
    "graphql",
    "cdn"
  ],
  "author": "Rene Volbach",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@apollo/server": "^4.7.0",
    "@apollo/subgraph": "^2.4.2",
    "@nestjs/apollo": "^11.0.5",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/graphql": "^11.0.5",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/testing": "^9.4.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "semantic-release": "^24.2.9",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "volbrene-git-hooks": "^1.9.2"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
